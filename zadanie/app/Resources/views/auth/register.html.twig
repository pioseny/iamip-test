{% extends 'base.html.twig' %}

{% block body %}
        <h2 class="form-signin-heading">Rejestracja</h2>

        {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_widget(form) }}

        <input type="hidden" name="_csrf_token"
               value="{{ csrf_token('authenticate') }}"
        >
        {{ form_end(form) }}
{% endblock %}